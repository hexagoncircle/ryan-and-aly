{
  "name": "Mulligabe",
  "version": "2021.0.5-18.0.6",
  "private": true,
  "license": "UNLICENSED",
  "homepage": "https://codepen.io/hexagoncircle/pen/f0150e7e2701b9b04f1c47adbfc955c7",
  "scripts": {
    "build": "gulp -f ./build/gulpfile.js --cwd ./ --continue",
    "serve": "gulp serve -f ./build/gulpfile.js --cwd ./ --continue",
    "task": "gulp -f ./build/gulpfile.js --cwd ./ --continue",
    "tasks": "gulp --tasks-simple -f ./build/gulpfile.js --cwd ./ --continue"
  },
  "config": {
    "srcDir": "src/",
    "distDir": "docs/"
  },
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "del": "^4.1.1",
    "glob": "^7.1.4",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^6.1.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-embed-svg": "^1.2.0",
    "gulp-inject-partials": "^1.0.5",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "node-sass": "^4.12.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "tasks": {
    "partials": {
      "order": 999,
      "src": [
        "**/*.template.html"
      ],
      "pipe": [
        {
          "require": "gulp-inject-partials",
          "args": [
            {
              "removeTags": true,
              "quiet": true,
              "ignoreError": true
            }
          ]
        }
      ],
      "rename": {
        "find": ".template",
        "replace": ""
      },
      "cleanup": [
        "./partials",
        "**/*.partial.html",
        "**/*.template.html"
      ]
    },
    "sass": {
      "src": [
        "./styles/**/*.scss"
      ],
      "pipe": [
        {
          "require": "gulp-sass",
          "dest": "./"
        }
      ],
      "cleanup": [
        "./styles/",
        "**/*.scss"
      ]
    },
    "autoprefixer": {
      "order": 999,
      "src": [
        "**/*.css"
      ],
      "pipe": [
        {
          "require": "gulp-autoprefixer",
          "args": [
            {
              "cascade": false
            }
          ]
        }
      ]
    },
    "minify-css": {
      "order": 999,
      "src": [
        "**/*.css"
      ],
      "pipe": [
        {
          "require": "gulp-clean-css"
        }
      ]
    }
  },
  "browserslist": [
    "last 5 version"
  ],
  "dependencies": {}
}
